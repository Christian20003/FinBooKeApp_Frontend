{
  "name": "financial-book-keeping-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:firefox": "ng test --no-watch --browsers=FirefoxHeadless",
    "test:chrome": "ng test --watch=false --browsers=ChromeHeadlessCustom",
    "build:ac": "ng build --prod",
    "prepare": "husky",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.3.15",
    "@angular/cdk": "^20.2.14",
    "@angular/cli": "^20.3.13",
    "@angular/common": "^20.3.15",
    "@angular/compiler": "^20.3.15",
    "@angular/core": "^20.3.15",
    "@angular/forms": "^20.3.15",
    "@angular/material": "^20.2.14",
    "@angular/platform-browser": "^20.3.15",
    "@angular/platform-browser-dynamic": "^20.3.15",
    "@angular/router": "^20.3.15",
    "@jsverse/transloco": "^8.2.0",
    "@ngrx/effects": "^20.1.0",
    "@ngrx/operators": "^20.1.0",
    "@ngrx/store": "^20.1.0",
    "d3": "^7.9.0",
    "rxjs": "^7.8.2",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.3.13",
    "@angular-eslint/builder": "20.7.0",
    "@angular-eslint/eslint-plugin": "20.7.0",
    "@angular-eslint/eslint-plugin-template": "20.7.0",
    "@angular-eslint/schematics": "20.7.0",
    "@angular-eslint/template-parser": "20.7.0",
    "@angular/compiler-cli": "^20.3.15",
    "@ngrx/eslint-plugin": "^20.1.0",
    "@types/d3": "^7.4.3",
    "@types/jasmine": "^5.1.13",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "jasmine-core": "^5.13.0",
    "karma": "^6.4.4",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.1",
    "karma-firefox-launcher": "^2.1.3",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "ng-mocks": "^14.15.0",
    "prettier": "^3.7.4",
    "prettier-eslint": "^16.4.2",
    "puppeteer": "^24.34.0",
    "typescript": "^5.9.3"
  },
  "overrides": {
    "@ngrx/effects": {
      "@angular/core": "$@angular/core"
    },
    "@ngrx/store": {
      "@angular/core": "$@angular/core"
    },
    "ng-mocks": {
      "@angular/core": "$@angular/core",
      "@angular/common": "$@angular/common",
      "@angular/forms": "$@angular/forms",
      "@angular/platform-browser": "$@angular/platform-browser"
    }
  }
}
